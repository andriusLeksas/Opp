package com.boardgame.GameWindow.Logic.StragegyPatternMovement;

import static org.junit.Assert.*;

import org.junit.Test;

import com.boardgame.GameWindow.Logic.StragegyPatternMovement.RedTankOperation;

public class RedTankOperationTest {

	@Test
	public void testDoOperation() {
		RedTankOperation test = new RedTankOperation();
		int ask = test.doOperation(9);
		assertEquals(ask, 8);
		int ask2 = test.doOperation(0);
		assertEquals(ask2, 0);
	}

}
